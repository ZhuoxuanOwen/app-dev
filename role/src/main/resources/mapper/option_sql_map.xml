<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- OptionDO Ibatis 映射关系配置文件 -->
<mapper namespace="com.zhuoxuan.vote.dao.OptionDao">

    <!-- 开发负责人：卓轩
    	 功能说明：OptionDO表查询 -->
	<insert id="addOption" parameterType="optionDO">
		INSERT INTO vote_option(survey_id,title,count,created)
			VALUES(#{surveyId},#{title},#{count},#{created})
	</insert>


</mapper>

